{
  "version": 2,
  "builds": [
    {
      "src": "server/inngest/index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**   "]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/inngest",
      "dest": "server/inngest/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "MONGODB_URI": "@mongodb_uri",
    "CLERK_SECRET_KEY": "@clerk_secret_key",
    "INNGEST_SIGNING_KEY": "@inngest_signing_key"
  },
  "functions": {
    "server/inngest/index.js": {
      "maxDuration": 30,
      "memory": 512
    }
  },
  "framework": null
}